#include "../src/Mvec.h"
#include "../src/Sym.h"

namespace gca {

    typedef Blade<int> Bladei;
    typedef Mvec<int>  Mveci;

    typedef Blade<long> Bladel;
    typedef Mvec<long>  Mvecl;

    typedef Blade<float> Bladef;
    typedef Mvec<float>  Mvecf;
    
    typedef Blade<double> Bladed;
    typedef Mvec<double>  Mvecd;
    
    typedef Blade<Sym> BladeS;
    typedef Mvec<Sym> MvecS;
    
}